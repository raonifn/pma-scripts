#!/bin/bash 

username=${1-$USER}

source pma_util

readPass "$username"

resp=$(curl -s -X post https://pma.dextra.com.br/services/invalidar_tokens -d username=$username\&password=$password)

handlePmaResponse "$resp"

if [ $response_code != '0' ]; then
   echo "$response_error"
   exit 1
fi

echo "$response_msgs"
